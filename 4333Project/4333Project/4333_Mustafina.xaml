<Window x:Class="_4333Project._4333_Mustafina"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_4333Project"
        mc:Ignorable="d"
        Title="_4333_Mustafina" Height="450" Width="800">
    <StackPanel VerticalAlignment="Center">
        <TextBlock HorizontalAlignment="Center" FontSize="30">Мустафина Зилия</TextBlock>
        <TextBlock HorizontalAlignment="Center" FontSize="30">18 лет</TextBlock>
        <TextBlock Text="Для работы с Excel:" Margin="0 30 0 0" HorizontalAlignment="Center"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button x:Name="ImportButton" Content="Импорт данных" Margin="0 10 30 0" Width="150" Click="ImportButton_Click"/>
            <Button x:Name="ExportButton" Content="Экспорт в Excel" Margin="30 10 0  0" Width="150" Click="ExportButton_Click"/>
        </StackPanel>
        <TextBlock Text="Для работы с Json:" Margin="0 30 0 0" HorizontalAlignment="Center"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button x:Name="JsonImportButton" Content="Импорт JSON данных" Margin="0 10 30 0" Width="150" Click="JsonImportButton_Click"/>
            <Button x:Name="JsonExportButton" Content="Экспорт в Word" Margin="30 10 0  0" Width="150" Click="JsonExportButton_Click"/>
        </StackPanel>
    </StackPanel>
</Window>
